      "type": "string",
      "required": true
    },
    "town": {
      "type": "string",
      "required": true
    },
    "postalCode": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
